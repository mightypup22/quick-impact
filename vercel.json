{
  "headers": [
    {"source": "/", "headers": [{"key": "Cache-Control", "value": "public, max-age=0, must-revalidate"}]},
    {"source": "/(.*).html", "headers": [{"key": "Cache-Control", "value": "public, max-age=0, must-revalidate"}]},
    {"source": "/components/(.*)", "headers": [{"key": "Cache-Control", "value": "public, max-age=0, must-revalidate"}]},
    {"source": "/(css|js)/(.*)", "headers": [{"key": "Cache-Control", "value": "public, max-age=600"}]},
    {"source": "/assets/(og|icons)/(.*)", "headers": [{"key": "Cache-Control", "value": "public, max-age=86400"}]}
  ],
  
  "version": 2,

  "functions": {
    "api/contact.js": {
      "runtime": "nodejs18.x"
    }
  }
}

